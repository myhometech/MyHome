modules = ["nodejs-20", "web", "postgresql-16"]
run = "npm run build && NODE_ENV=production node dist/index.js"
hidden = [".config", ".git", "generated-icon.png", "node_modules", "dist"]

[nix]
channel = "stable-24_05"

[deployment]
deploymentTarget = "autoscale"
run = ["sh", "-c", "node start-production-simple.js"]
build = ["sh", "-c", "npm run build"]

[[ports]]
localPort = 5000
externalPort = 80

[env]
PORT = "5000"

[workflows]
runButton = "Force Clean Development Server"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Start application"

[[workflows.workflow]]
name = "Start application"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 5000

[[workflows.workflow]]
name = "Start application with GC"
author = 45314489
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "NODE_ENV=development node --expose-gc --import tsx/esm server/index.ts"

[[workflows.workflow]]
name = "Production Deploy"
author = 45314489
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run build"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "NODE_ENV=production node --expose-gc dist/index.js"

[[workflows.workflow]]
name = "Start Production Server"
author = 45314489
mode = "sequential"

[[workflows.workflow.tasks]]
